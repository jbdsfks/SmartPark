<%include head%>
   <script src="/admin-controller.js"></script>
<body>
<div class="container-fluid" ng-controller="adminController">
  <div class="row-fluid">
    <div class="span12" style="color: white;background-color: black;">
      <h2 class="text-center">停车场数据管理系统</h2>
      <div class="navbar navbar-inverse">
        <div class="navbar-inner">
          <div class="container-fluid">
             <a data-target=".navbar-responsive-collapse" data-toggle="collapse" class="btn btn-navbar"><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></a>
             <div class="nav-collapse collapse navbar-responsive-collapse">
                <ul class="nav">
                  <li class="active"><a ng-click="Index()">主页</a></li>
                  <li class="dropdown">
                     <a data-toggle="dropdown" class="dropdown-toggle" href="#">停车场<strong class="caret"></strong></a>
                    <ul class="dropdown-menu">
                      <li><a ng-click="Allparks()">全部停车场</a></li>
                      <li><a ng-click="QuaryPark()">查询停车场</a></li>
                    </ul>
                  </li>
                   <li class="dropdown">
                     <a data-toggle="dropdown" class="dropdown-toggle" href="#">停车<strong class="caret"></strong></a>
                    <ul class="dropdown-menu">
                      <li><a ng-click="AllParkRecords()">全部停车信息</a></li>
                      <li><a ng-click="QuaryParkRecord()">查询停车信息</a></li>
                    </ul>
                  </li>
                  <li class="dropdown">
                     <a data-toggle="dropdown" class="dropdown-toggle" href="#">充值<strong class="caret"></strong></a>
                    <ul class="dropdown-menu">
                      <li><a ng-click="AllRecharges()">全部充值信息</a></li>
                      <li><a ng-click="QuaryRecharge()">查询充值信息</a></li>
                    </ul>
                  </li>
                </ul>
                <ul class="nav pull-right">
                  <li><a ng-click="logout()">注销登录</a></li>
                </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>




  <div class="row-fluid" ng-show="allparks">
    <center><h3>全部停车场信息</h3></center>
   <table  class="table table-striped">
          <tr>
              <th>编号</th>
              <th>名称</th>
              <th>地址</th>
              <th>现有车位(个)</th>
              <th>可用车位(个)</th>
              <th>收费标准(元/小时)</th>
              <th>联系电话</th>
              <th>管理员</th>
            </tr>
            <tr  ng-repeat="obj in list" ng-style="{'background-color':($index%2==0)? 'default':'#DCDCE6'}">
              <td ng-bind="obj.parkId">0</td>
              <td ng-bind="obj.name">0</td>
              <td ng-bind="obj.address">0</td>
              <td ng-bind="obj.size">0</td>
              <td ng-bind="obj.free">0</td>
              <td ng-bind="obj.price">0</td>
              <td ng-bind="obj.phone">0</td>
              <td ng-bind="obj.managerid">0</td>
            </tr>
      </table>
      <div class="page" style="color: blue;">共<span ng-bind="data.length"></span>条记录
              <span><span ng-click="divList(0)" class="link">首页</span><span ng-click="divList(-1)" ng-show="currentPage > 0" class="link">上一页</span><span ng-click="divList(1)" ng-show="currentPage!=totalPage" class="link">下一页</span>
            </div>
  </div>


  <div class="row-fluid" ng-show="quarypark">
    <br><br><center>
      <input id="parkid" type="text" autocomplete="off" placeholder="请输入停车场ID" ng-model="parkid" ng-click="clearErrorInfo()" style="width: 300px;height: 30px;font-size: 17px;margin-top: 10px;" />
      &nbsp;&nbsp;&nbsp;<button class="btn btn-success btn-lg" type="button" style="width: 50px;height: 40px;" ng-click="quaryParkInfo()">查询</button>
      <p class="text-danger" style="color: red;" ng-bind="errorInfo"></p></center>

      <div class="span9" ng-show="parkingByparkid" >
          <center><div class="page-header" style="font-size: 20px;" ng-bind="parkname"></div></center>
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>编号</th>
                  <th>用户</th>
                  <th>车牌号</th>
                  <th>进入时间</th>
                  <th>时长</th>
                  <th>金额</th>
                  <th>备注</th>
                </tr>
             </thead>
              <tbody>
                <tr  ng-repeat="p in list" ng-style="{'background-color':($index%2==0)? 'default':'#DCDCE6'}">
                  <td ng-bind="p.id">0</td>
                  <td ng-bind="p.user_id">0</td>
                  <td ng-bind="p.car_id">0</td>
                  <td ng-bind="p.car_in">0</td>
                  <td ng-bind="p.duration">0</td>
                  <td ng-bind="p.pay">0</td>
                  <td ng-bind="p.status">0</td>
                </tr>
              </tbody>
            </table>
            <div class="page" style="color: blue;">共<span ng-bind="data.length"></span>条记录
              <span><span ng-click="divList(0)" class="link">首页</span><span ng-click="divList(-1)" ng-show="currentPage > 0" class="link">上一页</span><span ng-click="divList(1)" ng-show="currentPage!=totalPage" class="link">下一页</span>
            </div>
          </div>
      </div>


  </div>


  <div class="row-fluid" ng-show="allparkrecords">
    <center><h3>全部停车信息</h3></center>
      <table class="table table-striped">
          <thead>
            <tr>
              <th>编号</th>
              <th>车牌号</th>
              <th>进入时间</th>
              <th>出来时间</th>
              <th>时长</th>
              <th>金额</th>
              <th>停车场</th>
            </tr>
         </thead>
          <tbody>
            <tr  ng-repeat="obj in list" ng-style="{'background-color':($index%2==0)? 'default':'#DCDCE6'}">
              <td ng-bind="$index+1">0</td>
              <td ng-bind="obj.carid">0</td>
              <td ng-bind="obj.intime">0</td>
              <td ng-bind="obj.outtime">0</td>
              <td ng-bind="obj.duration"></td>
              <td ng-bind="obj.pay">0</td>
              <td ng-bind="obj.parkname">0</td>
            </tr>
          </tbody>
        </table>
      <div class="page" style="color: blue;">共<span ng-bind="data.length"></span>条记录
              <span><span ng-click="divList(0)" class="link">首页</span><span ng-click="divList(-1)" ng-show="currentPage > 0" class="link">上一页</span><span ng-click="divList(1)" ng-show="currentPage!=totalPage" class="link">下一页</span>
            </div>
  </div>


  <div class="row-fluid" ng-show="quaryparkrecord">
   <br><br><center>
    <input id="userid" type="text" autocomplete="off" placeholder="请输入车牌号" ng-model="userid" ng-click="clearErrorInfo()" style="width: 300px;height: 30px;font-size: 17px;margin-top: 10px;" />&nbsp;&nbsp;&nbsp;
    <button class="btn btn-success btn-lg" type="button" style="width: 50px;height: 40px;" ng-click="quaryParkRecord()">查询</button><p class="text-danger" style="color: red;" ng-bind="errorInfo"></p></center>

  </div>

  <div class="row-fluid" ng-show="allrecharges">
    <center><h3>全部充值信息</h3></center>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>编号</th>
            <th>用户ID</th>
            <th>时间</th>
            <th>金额</th>
            <th>余额</th>
          </tr>
       </thead>
        <tbody>
          <tr  ng-repeat="obj in list" ng-style="{'background-color':($index%2==0)? 'default':'#DCDCE6'}">
            <td ng-bind="$index+1">0</td>
            <td ng-bind="obj.user_id">0</td>
            <td ng-bind="obj.time">0</td>
            <td ng-bind="obj.money">0</td>
            <td ng-bind="obj.account_balance">0</td>
          </tr>
        </tbody>
      </table>
      <div class="page" style="color: blue;">共<span ng-bind="data.length"></span>条记录
              <span><span ng-click="divList(0)" class="link">首页</span><span ng-click="divList(-1)" ng-show="currentPage > 0" class="link">上一页</span><span ng-click="divList(1)" ng-show="currentPage!=totalPage" class="link">下一页</span>
            </div>
  </div>
 
  <div class="row-fluid" ng-show="quaryrecharge">
        <br><br><center>
        <input id="userid" type="text" autocomplete="off" placeholder="请输入用户ID" ng-model="userid" ng-click="clearErrorInfo()" style="width: 300px;height: 30px;font-size: 17px;margin-top: 10px;" />&nbsp;&nbsp;&nbsp;
        <button class="btn btn-success btn-lg" type="button" style="width: 50px;height: 40px;" ng-click="quaryRecharge()">查询</button>
        <p class="text-danger" style="color: red;" ng-bind="errorInfo"></p></center>

       <div class="span9" ng-show="rechargeinfo" >
          <center><div class="page-header" style="font-size: 20px;" ng-bind="parkname"></div></center>
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>编号</th>
                  <th>用户ID</th>
                  <th>时间</th>
                  <th>金额</th>
                  <th>余额</th>
                </tr>
             </thead>
              <tbody>
                <tr  ng-repeat="obj in list" ng-style="{'background-color':($index%2==0)? 'default':'#DCDCE6'}">
                  <td ng-bind="$index+1">0</td>
                  <td ng-bind="obj.user_id">0</td>
                  <td ng-bind="obj.time">0</td>
                  <td ng-bind="obj.money">0</td>
                  <td ng-bind="obj.account_balance">0</td>
                </tr>
              </tbody>
            </table>
                <div class="page" style="color: blue;">共<span ng-bind="data.length"></span>条记录
                  <span ng-click="divList(0)" class="link">首页</span><span ng-click="divList(-1)" ng-show="currentPage > 0" class="link">上一页</span><span ng-click="divList(1)" ng-show="currentPage!=totalPage" class="link">下一页</span>
                </div>
          </div></div>

  </div>


</div>
</body>
</html>
